{
  "env": {
    "node": true,
    "mocha": true
  },
  "extends": "airbnb",
  "rules": {
    "func-names": 0,
    "no-new": 0,
    "no-use-before-define": 0,
    "no-script-url": 0,
    "no-alert": 0,
    "no-console": 0,
    "prefer-arrow-callback": 0,
    "arrow-body-style": 0,
  },
  "globals": {
  }
}
